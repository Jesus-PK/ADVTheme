	<x:String x:Key="FileChooser.FileSize">Tamaño del Archivo</x:String>
	
	<x:String x:Key="FileChooser.ShowQuickLinks">Mostrar Enlaces Rápidos</x:String>
	<x:String x:Key="FileChooser.HideQuickLinks">Ocultar Enlaces Rápidos</x:String>
	
	<x:String x:Key="FileChooser.ToolTip.QuickLinks">Muestra u Oculta las opciones de Navegación de los Enlaces Rápidos</x:String>
	<x:String x:Key="FileChooser.ToolTip.GoUp">Subir un Directorio</x:String>
	<x:String x:Key="FileChooser.ToolTip.Refresh">Recargar</x:String>
