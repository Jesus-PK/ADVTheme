	<x:String x:Key="Common.Medium">Media</x:String>
	<x:String x:Key="Common.Large">Grande</x:String>
	<x:String x:Key="Common.Name">Nombre</x:String>
	<x:String x:Key="Common.Hue">Tono</x:String>
