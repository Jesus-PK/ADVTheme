	<x:String x:Key="Common.Theme">Tema</x:String>
	<x:String x:Key="Common.Language">Idioma</x:String>
	<x:String x:Key="Common.FontSize">Tamaño de la Fuente</x:String>
	<x:String x:Key="Common.Small">Pequeña</x:String>
