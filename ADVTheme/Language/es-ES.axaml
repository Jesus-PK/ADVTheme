<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					x:ClassModifier="public">
	<!-- Any common strings to be shared across apps using these themes should be included here. -->
	<x:String x:Key="Common.OK">OK</x:String>
	<x:String x:Key="Common.Cancel">Cancelar</x:String>
	<x:String x:Key="Common.Back">Volver</x:String>
	<x:String x:Key="Common.Yes">Sí</x:String>
	<x:String x:Key="Common.No">No</x:String>
	<x:String x:Key="Common.Retry">Reintentar</x:String>
